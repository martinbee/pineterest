<div class="row">
  <% @pins.each do |pin| %>
    <div class="col-s-12 col-md-6 col-md-offset-3">
      <div class="thumbnail <%= pin.id %>-pin pin">
        <%= link_to (image_tag pin.photo.url(:pin), class: "pin-image"), [:open_pin_modal, id: pin.id], remote: true %>
      </div>
    </div>
  <% end %>
</div>
